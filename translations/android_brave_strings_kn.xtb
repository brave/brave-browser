<translation id="7507533586185621084">ತಪ್ಪಾದ ಸಿಂಕ್ ಕೋಡ್</translation>
<translation id="8284063834901328650">ತಪ್ಪಾದ ಸಿಂಕ್ QR ಕೋಡ್</translation>
<translation id="859506441384380749">ಈ ಕೋಡ್ ಅನ್ನು Brave ನ ಡೆಪ್ರಿಕೇಟ್ ಮಾಡಲಾದ ಆವೃತ್ತಿಯ ಮೂಲಕ ಜೆನರೇಟ್ ಮಾಡಲಾಗಿದೆ, ದಯವಿಟ್ಟು ಮೊದಲು ನಿಮ್ಮ ಮತ್ತೊಂದು ಸಾಧನವನ್ನು ಅಪ್‌ಗ್ರೇಡ್ ಮಾಡಿ</translation>
<translation id="3132642509209154999">ಈ ಕೋಡ್ ಅವಧಿ ಮೀರಿದೆ. ದಯವಿಟ್ಟು ಹೊಸತೊಂದನ್ನು ನಿಮ್ಮ ಮತ್ತೊಂದು ಸಾಧನದಲ್ಲಿ ಜೆನರೇಟ್ ಮಾಡಿ ಹಾಗೂ ಮತ್ತೊಮ್ಮೆ ಪ್ರಯತ್ನಿಸಿ.</translation>
<translation id="2859229680945501779">ಈ ಕೋಡ್ ಅಮಾನ್ಯವಾಗಿದೆ. ಸಮಯ ಮತ್ತು ಸಮಯವಲಯವನ್ನು ನಿಮ್ಮ ಸಾಧನದಲ್ಲಿ ಸರಿಯಾಗಿ ಹೊಂದಿಸಲಾಗಿದೆ ಎಂಬುದನ್ನು ದಯವಿಟ್ಟು ಪರಿಶೀಲಿಸಿ.</translation>
<translation id="3149880132460839085">ಬ್ರೇವ್‌  ಸಿಂಕ್</translation>
<translation id="3680755684186855422">ನಿಮ್ಮ ವಿವಿಧ ಸಾಧನಗಳಲ್ಲಿರುವ ನಿಮ್ಮ Brave ಬ್ರೌಸರ್‌ಗಳ ನಡುವೆ ಬುಕ್‌ಮಾರ್ಕ್‌ಗಳ ಡೇಟಾವನ್ನು ಖಾಸಗಿಯಾಗಿ ಸಿಂಕ್ ಮಾಡಲು ನಿಮಗೆ Brave ಸಿಂಕ್ ಅನುಮತಿಸುತ್ತದೆ.</translation>
<translation id="3735228155296883311">ನೀವು ಇನ್ನೊಂದು ಸಾಧನದಲ್ಲಿ ರಚಿಸಿದ ನಿಮ್ಮ ಸಿಂಕ್ ಚೈನ್‌ನಿಂದ ಕೋಡ್ ಅನ್ನು ಸ್ಕ್ಯಾನ್ ಮಾಡಿ. ಅಥವಾ ಹೊಸ ಸಿಂಕ್ ಚೈನ್‌ ಅನ್ನು ಪ್ರಾರಂಭಿಸಿ.</translation>
<translation id="2614069607859982576">ಅಸ್ತಿತ್ವದಲ್ಲಿರುವ ಸಿಂಕ್ ಮಾಡಲಾದ ಸಾಧನವನ್ನು ಬಳಸಿ, Brave ಸೆಟ್ಟಿಂಗ್ಗಳನ್ನು ತೆರೆಯಿರಿ ಮತ್ತು ಸೆಟ್ಟಿಂಗ್ಗಳಿಗೆ ನ್ಯಾವಿಗೇಟ್ ಮಾಡಿ &gt; ಸಿಂಕ್ ಎಂಬಲ್ಲಿಗೆ ನ್ಯಾವಿಗೇಟ್ ಮಾಡಿ. "ಹೊಸ ಸಾಧನವನ್ನು ಸೇರಿಸಿ" ಆಯ್ಕೆಮಾಡಿ ಮತ್ತು ಪರದೆಯ ಮೇಲೆ ಪ್ರದರ್ಶಿತವಾಗಿರುವ QR ಕೋಡ್‌ ಸ್ಕ್ಯಾನ್‌ ಮಾಡಿ.</translation>
<translation id="5241263629636958324">ಕ್ಲಿಪ್‌‌ಬೋರ್ಡ್‌ಗೆ ನಕಲಿಸಲಾಗಿದೆ</translation>
<translation id="5817918615728894473">ಜೋಡಿಸು</translation>
<translation id="7090325819305486658">ಡೇಟಾ ಆದ್ಯತೆಗಳು</translation>
<translation id="728334460283296316">ಸಾಧನ ತೆಗೆದುಹಾಕಿ</translation>
<translation id="1178128181015923849">ನೀವು ಸಾಧನವನ್ನು ಡಿಲೀಟ್‌ ಮಾಡಲು ಖಾತ್ರಿ ಹೊಂದಿರುವಿರೆ '<ph name="DEVICE_NAME"/>'</translation>
<translation id="7745082336226088126">ಸಿಂಕ್‌ ಕೋಡ್‌ ಸ್ಕ್ಯಾನ್‌ ಅಥವಾ ಎಂಟರ್‌ ಮಾಡಿ</translation>
<translation id="1602067095281698985">ಹೊಸ ಸಿಂಕ್ ಚೈನ್ ಅನ್ನು ಪ್ರಾರಂಭಿಸಿ</translation>
<translation id="1475223680121592371">ಒಂದು ಮೊಬೈಲ್‌ ಸಾಧನವನ್ನು ಸೇರಿಸಿ</translation>
<translation id="2965377893636908578">ಒಂದು ಕಂಪ್ಯೂಟರ್‌  ಅನ್ನು ಸೇರಿಸಿ</translation>
<translation id="6604289897677913048">ಎರಡನೆ ಫೋನ್‌ ಅಥವಾ ಟ್ಯಾಬ್ಲೆಟ್‌ ಬಳಸಿ, ಬ್ರೇವ್‌ ಸೆಟಿಂಗ್‌ಗೆ ನೇವಿಗೇಟ್‌ &gt; ಸಿಂಕ್‌ &gt; ಸ್ಕ್ಯಾನ್‌.</translation>
<translation id="8508856434435157945">ನಿಮ್ಮ ಮೊಬೈಲ್ ಸಾಧನದಲ್ಲಿ, ಸೆಟ್ಟಿಂಗ್‌ಗಳ ಫಲಕದಲ್ಲಿ ಬ್ರೇವ್ ಸಿಂಕ್‌ಗೆ ನ್ಯಾವಿಗೇಟ್ ಮಾಡಿ ಮತ್ತು "ಸ್ಕ್ಯಾನ್ ಸಿಂಕ್ ಕೋಡ್" ಬಟನ್ ಕ್ಲಿಕ್ ಮಾಡಿ. ಕೆಳಗಿನ ಕ್ಯುಆರ್‌ ಕೋಡ್ ಅನ್ನು ಸ್ಕ್ಯಾನ್ ಮಾಡಲು ನಿಮ್ಮ ಕ್ಯಾಮೆರಾ ಬಳಸಿ</translation>
<translation id="2590683710114839102">ನಿಮ್ಮ ಕಂಪ್ಯೂಟರ್‌ನಲ್ಲಿ, ಬ್ರೇವ್‌ ಸೆಟ್ಟಿಂಗ್‌ಗೆ ನೇವಿಗೇಟ್‌ ಮಾಡಿ &gt; ಸಿಂಕ್‌. ಆಯ್ಕೆ ಮಾಡಿಕೊಳ್ಳಿ "ಸಿಂಕ್‌ ಚೈನ್‌ ಕೋಡ್‌ ಎಂಟರ್‌ ಮಾಡಿ".</translation>
<translation id="65315202420929965">ಈ ಸಿಂಕ್‌ ಕೋಡ್‌ ಅನ್ನು ಸ್ಕ್ಯಾನ್‌ ಮಾಡಿ</translation>
<translation id="6764016175302339802">ಈ ಕೋಡ್ ಅನ್ನು ಪಾಸ್‌ವರ್ಡ್‌ನಂತೆ ಪರಿಗಣಿಸಿ. ಯಾರಾದರೂ ಅದನ್ನು ಹಿಡಿದಿಟ್ಟುಕೊಂಡಿದ್ದರೆ, ಅವರು ನಿಮ್ಮ ಸಿಂಕ್ ಮಾಡಿದ ಡೇಟಾವನ್ನು ಓದಬಹುದು ಮತ್ತು ಮಾರ್ಪಡಿಸಬಹುದು.</translation>
<translation id="6707003343517029414">ನಿಮ್ಮ ಉದ್ದೇಶಿತ ಕಂಪ್ಯೂಟರ್‌ನಲ್ಲಿ, ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ಬ್ರೇವ್ ಸಿಂಕ್‌ಗೆ ನ್ಯಾವಿಗೇಟ್ ಮಾಡಿ ಮತ್ತು "ನಾನು ಸಿಂಕ್ ಕೋಡ್ ಹೊಂದಿರುವೆ" ಬಟನ್ ಕ್ಲಿಕ್ ಮಾಡಿ. \nಈ ಕೆಳಗೆ ತೋರಿಸಿರುವ ಸಿಂಕ್ ಚೈನ್ ಕೋಡ್ ಪದಗಳನ್ನು ನಮೂದಿಸಿ.</translation>
<translation id="2731921493694251495">ಸಿಂಕ್‌ ಚೈನ್‌ ಕೋಡ್‌ ಎಂಟರ್‌ ಮಾಡಿ</translation>
<translation id="2175927920773552910">QR ಕೋಡ್</translation>
<translation id="8653433343015102968">ಕೋಡ್‌ ವರ್ಡ್‌ಗಳು</translation>
<translation id="8624315169751085215">ಕ್ಲಿಪ್‌ಬೋರ್ಡ್‌ಗೆ ಕಾಪಿ ಮಾಡಿ</translation>
<translation id="8730621377337864115">ಮುಗಿದಿದೆ</translation>
<translation id="6174119914106813304">ಈ ಸಾಧನದ ಮೇಲೆ ಸಿಂಕ್‌ ಆಗಿದೆ</translation>
<translation id="6776430845975754238">ಸೆಟ್ಟಿಂಗ್‌ನಲ್ಲಿನ ಬದಲಾವಣೆ ಈ ಸಾಧನವು ಇತತರೊಂದಿಗೆ ಹಂಚಿಕೊಳ್ಳುವ ಡಾಟಾಗಳ ಮೇಲೆ ಮಾತ್ರ ಪರಿಣಾಮ ಬೀರುತ್ತದೆ.</translation>
<translation id="1061533051865606843">ಹೊಸ ಸಾಧನವನ್ನು ಸೇರಿಸಿ</translation>
<translation id="7539985410310934287">ಕೋಡ್‌ ವರ್ಡ್‌ಗಳನ್ನು ನಮೂದಿಸಿ</translation>
<translation id="2886391679017857869">ಅಸ್ತಿತ್ವದಲ್ಲಿರುವ ಸಿಂಕ್ ಮಾಡಲಾದ ಸಾಧನವನ್ನು ಬಳಸಿ, Brave ಸೆಟ್ಟಿಂಗ್ಗಳನ್ನು ತೆರೆಯಿರಿ ಮತ್ತು ಸೆಟ್ಟಿಂಗ್ಗಳಿಗೆ ನ್ಯಾವಿಗೇಟ್ ಮಾಡಿ &gt; ಸಿಂಕ್ ಎಂಬಲ್ಲಿಗೆ ನ್ಯಾವಿಗೇಟ್ ಮಾಡಿ. "ಹೊಸ ಸಾಧನ ಸೇರಿಸಿ" ನಂತರ "ಬದಲಿಗೆ ಕೋಡ್‌ ವರ್ಡ್‌‌ಗಳನ್ನು ಪ್ರದರ್ಶಿಸಿ" ಅನ್ನು ಆರಿಸಿ.</translation>
<translation id="8131740175452115882">ದೃಢೀಕರಿಸು</translation>
<translation id="4973653569293835178">ನಾನು ನನ್ನ ಕ್ಯಾಮೆರಾ ಬಳಸುತ್ತೇನೆ</translation>
<translation id="326454632686131209">ಸಾಧನ ಸಿಂಕ್‌ ಮಾಡುವುದು</translation>
<translation id="327726072512457161">ಸೆಟ್ಟಿಂಗ್ಸ್‌ ನಿರ್ಗಮನ</translation>
<translation id="8050847734948770789">Brave Rewards ಸಕ್ರಿಯಗೊಳಿಸಿ</translation>
<translation id="9187185473011056079">ನಾನು ಬಂದಿರುವೆ!</translation>
<translation id="8185825081026121627">ಗೌಪ್ಯತೆ ಸಂರಕ್ಷಿಸುವ Brave ಪುರಸ್ಕಾರಗಳ ಜಾಹೀರಾತುಗಳನ್ನು ನೋಡುವುದಕ್ಕಾಗಿ ಬೇಸಿಕ್ ಅಟೆನ್ಶನ್ ಟೋಕನ್ ಸಂಪಾದಿಸಿ.</translation>
<translation id="5321518976657560663">ಎರರ್‌ ರಿಸೆಟ್‌ ಮಾಡಿ</translation>
<translation id="4114170343590425996">ಬ್ರೇವ್‌ ರಿವಾರ್ಡ್‌ ಮರು ಸೆಟ್ಟಿಂಗ್‌ನಲ್ಲಿ ದೋಷವಿದೆ.  ಬ್ರೇವ್‌ ರೀಲಾಂಚ್‌ ಮಾಡಿ ಮತ್ತು ಮತ್ತೊಮ್ಮೆ ಪ್ರಯತ್ನಿಸಿ</translation>
<translation id="6393502589311632553">ಬ್ರೇವ್‌ ಬಳಕೆಯಲ್ಲಿ ಇಲ್ಲದಾಗ ಜಾಹೀರಾತು ಪ್ರದರ್ಶಿಸುತ್ತದೆ</translation>
<translation id="3349204812119681938">ಬ್ರೇವ್‌ ರಿವಾರ್ಡ್‌ಗಳನ್ನು ರಿಸೆಟ್‌ ಮಾಡಿ</translation>
<translation id="8782039523249932839">ಇದು ನಿಮ್ಮ ಬ್ರೇವ್‌ ವ್ಯಾಲೆಟ್‌ ಅನ್ನು ಡಿಲೀಟ್‌ ಮಾಡುತ್ತದೆ ಮತ್ತು ಎಲ್ಲ ಬ್ರೇವ್‌ ರಿವಾರ್ಡ್‌ ದಾಖಲೆಗಳನ್ನು ಮರು ಜೋಡಣೆ ಮಾಡುತ್ತದೆ.</translation>
<translation id="6810078320346589989">ಬೂಕ್‌ಮಾರ್ಕ್‌ ಸೃಷ್ಟಿಸಿ</translation>
<translation id="3396347744975515056">Brave ಸಿಂಕ್ ಅನ್ನು ನವೀಕರಿಸಲಾಗಿದೆ ಮತ್ತು ಸೆಟಪ್ ಅಗತ್ಯವಿದೆ</translation>
<translation id="7473245612074035141">ಸೆಟಪ್ ಆರಂಭಿಸಿ</translation>
<translation id="8985635554450124959">ಸಿಂಕ್ ಅನ್ನು ಸೆಟಪ್ ಮಾಡಲು ಸಾಧ್ಯವಿಲ್ಲ</translation>
<translation id="2514300153301737214">ನೀವು Brave ಸಿಂಕ್ ಅನ್ನು ಸಕ್ರಿಯಗೊಳಿಸಿದ್ದೀರಿ ಎಂದು ನಾವು ಪತ್ತೆ ಮಾಡಿದ್ದೇವೆ, ಹಾಗಿದ್ದರೂ ನಿಮ್ಮ ಸಿಸ್ಟಮ್ ಸಿಂಕ್ ಆಫ್ ಮಾಡಲಾಗಿದೆ ಮತ್ತು ಅದು Brave ಅನ್ನು ಸಿಂಕ್ ಮಾಡಲು ಅನುಮತಿಸುವುದಿಲ್ಲ. ಸೆಟ್ಟಿಂಗ್‌ಗಳನ್ನು ತೆರೆಯುವುದಕ್ಕಾಗಿ ಒತ್ತಿ.</translation>
<translation id="7173114856073700355">ಸೆಟ್ಟಿಂಗ್‌ಗಳನ್ನು ತೆರೆಯಿರಿ</translation>
<translation id="6965382102122355670">ಸರಿ</translation>
<translation id="4450893287417543264">ಮತ್ತೆ ತೋರಿಸಬೇಡಿ</translation>
<translation id="1056898198331236512">ಎಚ್ಚರಿಕೆ</translation>
<translation id="1426410128494586442">ಹೌದು</translation>
<translation id="7811264919113243207">ಈ QR ಕೋಡ್ ಅನ್ನು ಜೆನರೇಟ್ ಮಾಡಿದ ಸಾಧನದೊಂದಿಗೆ ಸಂಭಾವ್ಯವಾಗಿ ನಿಮ್ಮ ಪಾಸ್‌ವರ್ಡ್‌ಗಳನ್ನು ಒಳಗೊಂಡಂತೆ ನಿಮ್ಮ Brave ಡೇಟಾವನ್ನು ಇದು ಹಂಚಿಕೊಳ್ಳುತ್ತದೆ.
ಈ QR ಕೋಡ್ ಅನ್ನು ನೀವು ಹೊಂದಿರುವ ಸಾಧನವು ಜೆನರೇಟ್ ಮಾಡಿದೆಯೇ ಎಂಬುದನ್ನು ದಯವಿಟ್ಟು ಎರಡು ಬಾರಿ ಪರಿಶೀಲಿಸಿ.
ಇದನ್ನು ಮಾಡಲು ನೀವು ಖಚಿತವಾಗಿ ಬಯಸುವಿರಾ?</translation>
<translation id="3097089005389027085">ಸಂಭಾವ್ಯವಾಗಿ ನಿಮ್ಮ ಪಾಸ್‌ವರ್ಡ್‌ಗಳನ್ನು ಒಳಗೊಂಡಂತೆ ನಿಮ್ಮ Brave ಡೇಟಾವನ್ನು ಈ ಕೋಡ್ ಪದಗಳನ್ನು ಜೆನರೇಟ್ ಮಾಡಿರುವ ಸಾಧನದೊಂದಿಗೆ ಇದು ಹಂಚಿಕೊಳ್ಳುತ್ತದೆ.
ಈ ಕೋಡ್ ಪದಗಳನ್ನು ನೀವು ಹೊಂದಿರುವ ಸಾಧನವು ಜೆನರೇಟ್ ಮಾಡಿದೆ ಎಂಬುದನ್ನು ದಯವಿಟ್ಟು ಎರಡು ಬಾರಿ ಪರಿಶೀಲಿಸಿ.
ಇದನ್ನು ಮಾಡಲು ನೀವು ಖಚಿತವಾಗಿ ಬಯಸುವಿರಾ?</translation>
<translation id="597993392929850353">Brave ಪರವಾನಗಿ</translation>
<translation id="6336451774241870485">ಹೊಸ ಖಾಸಗಿ ಟ್ಯಾಬ್‌</translation>
<translation id="6199596294742117349">ಇದು ಖಾಸಗಿ ಟ್ಯಾಬ್ ಆಗಿದೆ</translation>
<translation id="5858455511731621847">ಖಾಸಗಿ ಟ್ಯಾಬ್‌ನಲ್ಲಿ ನೀವು ಭೇಟಿ ನೀಡುವ ಸೈಟ್‌ಗಳನ್ನು ಸ್ಥಳೀಯವಾಗಿ ಉಳಿಸುವುದಿಲ್ಲವಾದರೂ, ಅವುಗಳು ನಿಮ್ಮನ್ನು ಅನಾಮಧೇಯಗೊಳಿಸುವುದಿಲ್ಲ ಅಥವಾ ನಿಮ್ಮ ISP, ಉದ್ಯೋಗದಾತರು ಅಥವಾ ನೀವು ಭೇಟಿ ನೀಡುತ್ತಿರುವ ಸೈಟ್‌ಗಳಿಗೆ ಕಾಣುವುದಿಲ್ಲ.</translation>
<translation id="5789441016350810005">ಬ್ರೌಸರ್ ಹಿನ್ನೆಲೆಯಲ್ಲಿರುವಾಗ Brave ಜಾಹೀರಾತುಗಳನ್ನು ಕಳುಹಿಸಲಾಗಿದೆ</translation>
<translation id="3690140184638701871">ಕೊಡುಗೆಗಳು &amp; ಸಲಹೆಗಳು</translation>
<translation id="844921671123185605">ನಿಮ್ಮ ಮಾಸಿಕ ಸುಳಿವುಗಳನ್ನು ಪ್ರಕ್ರಿಯೆಗೊಳಿಸಲಾಗಿದೆ!</translation>
<translation id="3124090095880236567">ಜಾಹೀರಾತುಗಳನ್ನು ಆನ್ ಮಾಡಿ</translation>
<translation id="2975780980638932742">Brave ಜಾಹೀರಾತುಗಳು ತಲುಪಿವೆ!</translation>
<translation id="7541582550122501404">ಕೊಡುಗೆ ಬಾಕಿ ಉಳಿದಿದೆ</translation>
<translation id="8520059774736886747">ರಚನೆಕಾರರು <ph name="PUBLISHER_NAME"/> ಇತ್ತೀಚಿಗೆ ಪರಿಶೀಲಿಸಲಾಗಿದೆ</translation>
<translation id="7924320838799747606">ನಿಮ್ಮ ಬ್ಯಾಲೆನ್ಸ್</translation>
<translation id="1486343603936278248">ಟೋಕನ್‌</translation>
<translation id="7781069478569868053">ಹೊಸ ಟ್ಯಾಬ್ ಪುಟ</translation>
<translation id="8522669544471808198">ಹಿನ್ನೆಲೆ ಚಿತ್ರಗಳನ್ನು ತೋರಿಸಿ</translation>
<translation id="3870475560794762985">ಪ್ರಾಯೋಜಿತ ಚಿತ್ರಗಳನ್ನು ತೋರಿಸಿ</translation>
<translation id="8297488609722895137"><ph name="PHOTO_BY"/> ಮೂಲಕ ಫೋಟೋ</translation>
<translation id="4761229094276239865">Brave Rewards ಬಳಸುವುದನ್ನು ಪ್ರಾರಂಭಿಸು ಕ್ಲಿಕ್ ಮಾಡುವ ಮೂಲಕ, <ph name="TERMS_OF_SERVICE"/> ಗೆ ನೀವು ಸಮ್ಮತಿಸುತ್ತೀರಿ.</translation>
<translation id="3794079107601824147">ಈ ಹಿನ್ನೆಲೆ ಚಿತ್ರವನ್ನು ವೀಕ್ಷಿಸಲು ಪಾವತಿ ಪಡೆಯಿರಿ.</translation>
<translation id="7136586167396205364">ಪ್ರಾಯೋಜಿತ ಚಿತ್ರಗಳನ್ನು ಮರೆಮಾಡಿ</translation>
<translation id="2927095357213611519">ನೀವು ಟೋಕನ್‌ಗಳೊಂದಿಗೆ ವೆಬ್ ರಚನೆಕಾರರನ್ನು ಬೆಂಬಲಿಸಿ.\nಗೌಪ್ಯತೆ-ಗೌರವಿಸುವ ಜಾಹೀರಾತುಗಳನ್ನು ವೀಕ್ಷಿಸುವ ಮೂಲಕ ಟೋಕನ್‌ಗಳನ್ನು ಸಂಪಾದಿಸಿ.</translation>
<translation id="213787746370741250">ಕಸ್ಟಮ್ ಟ್ಯಾಬ್‌ಗಳು ರಲ್ಲಿ ಟ್ಯಾಬ್‌ಗಳನ್ನು ತೆರೆಯಿರಿ</translation>
<translation id="8030169304546394654">ಸಂಪರ್ಕ ಕಡಿತಗೊಳಿಸಲಾಗಿದೆ</translation>
<translation id="6181762795539107980">ಪರಿಶೀಲಿಸಿಲ್ಲದವು</translation>
<translation id="543510923939676996">ವ್ಯಾಲೆಟ್ ಪರಿಶೀಲಿಸಲಾಗಿದೆ</translation>
<translation id="8673913984316081937">ನಿಮ್ಮ ವ್ಯಾಲೆಟ್ ಪರಿಶೀಲಿಸುವುದು ಐಚ್ಛಿಕವಾಗಿದೆ</translation>
<translation id="8558636200953226885">ಆದರೆ ನೀವು ದೃಢೀಕರಿಸಿದರೆ, ಹೀಗೆ ಮಾಡಬಹುದು...</translation>
<translation id="8697510149346635496">ಗೌಪ್ಯತೆ-ಗೌರವಿಸುವ ಜಾಹೀರಾತುಗಳನ್ನು ನೋಡುವುದರಿಂದ ನೀವು ಗಳಿಸುವ BAT ಅನ್ನು ವಿತ್‌ಡ್ರಾ ಮಾಡಿಕೊಳ್ಳಿ</translation>
<translation id="1481480653609649134">ಕ್ರೆಡಿಟ್ ಕಾರ್ಡ್‌ಗಳು ಮತ್ತು ಇತರ ಮೂಲಗಳನ್ನು ಬಳಸಿಕೊಂಡು ಹೆಚ್ಚುವರಿ BAT ಖರೀದಿಸಿ</translation>
<translation id="9096003370755878766">ನಿಮ್ಮ Brave ಬಹುಮಾನಗಳ ವಾಲೆಟ್‌ಗೆ ನೀವು ಈ ಹಿಂದೆ ಸೇರಿಸಿದ್ದ BAT ಅನ್ನು ವಿತ್‌ಡ್ರಾ ಮಾಡಿಕೊಳ್ಳಿ</translation>
<translation id="1554282356558968486">ನಮ್ಮ ವ್ಯಾಲೆಟ್ ಸೇವೆಯನ್ನು ಒದಗಿಸಿದವರು</translation>
<translation id="6569877533335686968">ಅಪ್‌ಹೋಲ್ಡ್</translation>
<translation id="734105583050813665">ಬಿಟ್‌ಫ್ಲೈಯರ್</translation>
<translation id="8533411848724153701">ಜೆಮಿನಿ</translation>
<translation id="8465442795593991224">ವಿನಂತಿಸಿದ ಸೇವೆಗಳ ಆಧಾರದ ಮೇಲೆ ನಿಮ್ಮ ಗುರುತನ್ನು ಪರಿಶೀಲಿಸಲು ಅಪ್‌ಹೋಲ್ಡ್ ನಿಮಗೆ ಅಗತ್ಯವಿರಬಹುದು.</translation>
<translation id="3200197118442451932">ನೀವು Brave Software Inc. ಜೊತೆಗೆ ಖಾತೆ ಪ್ರಾರಂಭಿಸಿದಾಗ ನೀವು <ph name="WALLET_PROVIDER"/> ಗೆ ಒದಗಿಸಿದ ಯಾವುದೇ ವೈಯಕ್ತಿಕ ಮಾಹಿತಿಯನ್ನು ಅದು ಪ್ರಕ್ರಿಯೆಗೊಳಿಸುವುದಿಲ್ಲ, ಸಂಗ್ರಹಿಸುವುದಿಲ್ಲ ಅಥವಾ ಅದನ್ನು ಪ್ರವೇಶಿಸುವುದಿಲ್ಲ.</translation>
<translation id="3699674790322420653">ವ್ಯಾಲೆಟ್ ಪರಿಶೀಲನೆ ಪೂರ್ಣಗೊಳಿಸಿ</translation>
<translation id="2193391216824305230">ಬಳಕೆದಾರ ವ್ಯಾಲೆಟ್</translation>
<translation id="1665118090708049096">ನನ್ನ <ph name="WALLET_PROVIDER"/> ಖಾತೆಗೆ ಹೋಗಿ</translation>
<translation id="1948959999802743233"><ph name="WALLET_PROVIDER"/> ಇಂದ ಸಂಪರ್ಕ ಕಡಿತಗೊಳಿಸಿ</translation>
<translation id="6239274080752256452">ಹಣವನ್ನು ಹಿಂಪಡೆಯಿರಿ</translation>
<translation id="8874184842967597500">ಸಂಪರ್ಕಗೊಳಿಸಿಲ್ಲ</translation>
<translation id="3368922792935385530">ಸಂಪರ್ಕಿಸಲಾಗಿದೆ</translation>
<translation id="4569155249847375786">ಪರಿಶೀಲಿಸಲಾಗಿದೆ</translation>
<translation id="5358719431303818601">ಸಂಪರ್ಕ ಕಡಿತವನ್ನು ಪರಿಶೀಲಿಸಲಾಗಿಲ್ಲ</translation>
<translation id="4241449868720191203">ಸಂಪರ್ಕ ಕಡಿತವನ್ನು ಪರಿಶೀಲಿಸಲಾಗಿದೆ</translation>
<translation id="3248185426436836442">ಬಾಕಿ ಉಳಿದಿರುವುದು</translation>
<translation id="4488355059795908907">Brave ಬಳಕೆದಾರರಿಂದ ಕೊಡುಗೆಗಳನ್ನು ಸ್ವೀಕರಿಸಲು ಈ ಪರಿಶೀಲಿತ ಈ ಪರಿಶೀಲಿತ Brave ರಚನೆಕಾರ ಇನ್ನೂ ಕಾನ್ಫಿಗರ್ ಮಾಡಿಲ್ಲ. ನೀವು ಕಳುಹಿಸುವ ಯಾವುದೇ ಸಲಹೆಗಳು ಈ ಪ್ರಕ್ರಿಯೆಯನ್ನು ಪೂರ್ಣಗೊಳಿಸುವವರೆಗೆ ನಿಮ್ಮ ವ್ಯಾಲೆಟ್‌ನಲ್ಲಿ ಉಳಿಯುತ್ತವೆ.</translation>
<translation id="2495032782748793764">Brave ಡೀಫಾಲ್ಟ್</translation>
<translation id="9132378787893584761">ನಿಮ್ಮ ಸ್ನೇಹಿತರೊಂದಿಗೆ Brave ಹಂಚಿಕೊಳ್ಳಿ!</translation>
<translation id="717952606991220586">ಓಹೋ, ಏನೋ ತಪ್ಪಾಗಿದೆ. ದಯವಿಟ್ಟು ನಂತರ ಮತ್ತೊಮ್ಮೆ ಪ್ರಯತ್ನಿಸಿ.</translation>
<translation id="2524310205387880526">Brave ರೇಟ್ ಮಾಡಿ</translation>
<translation id="987264212798334818">ಸಾಮಾನ್ಯ</translation>
<translation id="1116694919640316211">ಕುರಿತು</translation>
<translation id="6680650203439190394">ದರ</translation>
<translation id="2635089463885398597">Brave ಅವರೊಂದಿಗಿನ ನಿಮ್ಮ ಅನುಭವವನ್ನು ನೀವು ಹೇಗೆ ರೇಟ್ ಮಾಡುತ್ತೀರಿ?</translation>
<translation id="141901961143729572">ನಿಮ್ಮ ಆಲೋಚನೆಗಳನ್ನು ಹಂಚಿಕೊಳ್ಳಿ</translation>
<translation id="7961015016161918242">ಎಂದಿಗೂ ಇಲ್ಲ</translation>
<translation id="1275213816424141989">ಬಹುಶಃ ನಂತರ</translation>
<translation id="6991128190741664836">ನಂತರ</translation>
<translation id="8851456134224363571">ಅದ್ಭುತವಾಗಿದೆ! ಪ್ಲೇ ಸ್ಟೋರ್‌ನಲ್ಲಿ ನಮಗೆ ರೇಟಿಂಗ್ ನೀಡಲು ನೀವು ಬಯಸುವಿರಾ?</translation>
<translation id="8668962040900592974">ನಾವು ಹೇಗೆ ಸುಧಾರಿಸಬಹುದು ಎಂದು ನಮಗೆ ತಿಳಿಸಿ</translation>
<translation id="4406937629441301400">ಈ ಚಿತ್ರವನ್ನು ವೀಕ್ಷಿಸಲು ಟೋಕನ್‌ಗಳನ್ನು ಸಂಪಾದಿಸಿ ಮತ್ತು ಅದನ್ನು ರಚನೆಕಾರರಿಗೆ ಫಾರ್ವಡ್ ಮಾಡಲು ಪಾವತಿಸಿ. <ph name="LEARN_MORE"/></translation>
<translation id="1992363542751124282">ಈ ಚಿತ್ರವನ್ನು ನೋಡುವುದಕ್ಕಾಗಿ ಟೋಕನ್‌ಗಳನ್ನು ಸಂಪಾದಿಸಿ. ನಿಮ್ಮ ಪಾಲನ್ನು ಪಡೆಯಲು Brave Rewards ಬಳಸಲು ಪ್ರಾರಂಭಿಸು ಟ್ಯಾಪ್ ಮಾಡಿ.</translation>
<translation id="6632345639591919543">ಈ ಇಮೇಜ್ ಅನ್ನು ವೀಕ್ಷಿಸಲು ನೀವು ಟೋಕನ್‌ಗಳನ್ನು ಸಂಪಾದಿಸುತ್ತಿದ್ದೀರಿ.\n<ph name="LEARN_MORE"/></translation>
<translation id="1465733111003593696">ಈ ಇಮೇಜ್ ಅನ್ನು ವೀಕ್ಷಿಸಲು ನೀವು ಟೋಕನ್‌ಗಳನ್ನು ಗಳಿಸುತ್ತಿದ್ದೀರಿ.</translation>
<translation id="7877201489035217175">ಪ್ರಾಯೋಜಿತ ಇಮೇಜ್‌ಗಳ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ</translation>
<translation id="828092975561478703">ನೀವು ವೀಕ್ಷಿಸುವ ಪ್ರತಿ ಪ್ರಾಯೋಜಿತ ಚಿತ್ರಕ್ಕಾಗಿ, ನೀವು 70% ಆದಾಯವನ್ನು ಟೋಕನ್‌ಗಳಲ್ಲಿ ಸ್ವೀಕರಿಸುತ್ತೀರಿ. Brave ಜಾಹೀರಾತುಗಳೊಂದಿಗೆ, ನೀವು ನೋಡುವ ಜಾಹೀರಾತುಗಳ ನಿಯಂತ್ರಣವನ್ನು ನೀವು ಯಾವಾಗಲೂ ಹೊಂದಿರುತ್ತೀರಿ.</translation>
<translation id="3624048886326677117">ಟ್ರ್ಯಾಕರ್‌ಗಳು &amp; ಜಾಹೀರಾತುಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ (ಆಕ್ರಮಣಶೀಲ ವಿಧಾನ)</translation>
<translation id="7151914815476509258">ಟ್ರ್ಯಾಕರ್‌ಗಳು &amp; ಜಾಹೀರಾತುಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ (ಪ್ರಮಾಣಿತ)</translation>
<translation id="7384778069178817075">ಎಲ್ಲಾ ಟ್ರ್ಯಾಕರ್‌ಗಳು &amp; ಜಾಹೀರಾತುಗಳನ್ನು ಅನುಮತಿಸಿ</translation>
<translation id="4941539331405513605">ಕ್ರಾಸ್-ಸೈಟ್ ಕುಕೀಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ</translation>
<translation id="3763681089644577874">ಕುಕೀಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ</translation>
<translation id="614747795780742163">ನಿಮ್ಮ ಹಿಂದಿನ ಭೇಟಿಗಳ ಬಗ್ಗೆ ಮಾಹಿತಿಯನ್ನು ಸಂಗ್ರಹಿಸುವುದರಿಂದ ವೆಬ್‌ಸೈಟ್‌ಗಳನ್ನು ತಡೆಯುತ್ತದೆ</translation>
<translation id="6883611015375728278">ಎಲ್ಲಾ ಕುಕೀಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ</translation>
<translation id="5657156137487675418">ಎಲ್ಲಾ ಕುಕೀಗಳನ್ನು ಅನುಮತಿಸಿ</translation>
<translation id="3895527457258986253">ಫಿಂಗರ್‌ಪ್ರಿಂಟಿಂಗ್‌ಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ</translation>
<translation id="7854108565842971747">ಇವೆಲ್ಲಾ ಹೊಸ ಸೈಟ್‍ಗಳಿಗೆ ಲಭ್ಯವಿರುವ ಡೀಫಾಲ್ಟ್ ಶೀಲ್ಡ್ ಸೆಟ್ಟಿಂಗ್‍ಗಳಾಗಿವೆ. ಇವನ್ನು ಬದಲಿಸುವುದಿಂದ ನಿಮ್ಮ ಸದ್ಯದ ಪ್ರತಿ-ಸೈಟ್ ಸೆಟ್ಟಿಂಗ್‍ಗಳ ಮೇಲೆ ಯಾವುದೇ ಪರಿಣಾಮ ಬೀರುವುದಿಲ್ಲ.</translation>
<translation id="184568144098869637">ನಿಮ್ಮ ಸಾಧನವನ್ನು ಗುರುತಿಸುವುದನ್ನು ಬ್ರೌಸರ್ ತಡೆಯುತ್ತದೆ</translation>
<translation id="7445306466605902444">ಫಿಂಗರ್‌ಪ್ರಿಂಟಿಂಗ್ ನಿರ್ಬಂಧಿಸಲಾಗಿದೆ (ಕಟ್ಟುನಿಟ್ಟಾದ, ಸೈಟ್‌ಗಳನ್ನು ವಿಂಗಡಿಸಬಹುದು)</translation>
<translation id="5582686930349810484">ಫಿಂಗರ್‌ಪ್ರಿಂಟಿಂಗ್ ನಿರ್ಬಂಧಿಸಲಾಗಿದೆ (ಪ್ರಮಾಣಿತ)</translation>
<translation id="769702552748212954">ಎಲ್ಲಾ ಫಿಂಗರ್‌ಪ್ರಿಂಟಿಂಗ್‌ ಅನುಮತಿಸಿ</translation>
<translation id="876424292214881779">ಜಾಹೀರಾತುಗಳು ಮತ್ತು ಇತರ ಅಹಿತಕರ ಸಂಗತಿಗಳನ್ನು ನಿರ್ಬಂಧಿಸಲಾಗಿದೆ</translation>
<translation id="6930134873084561197">ಸೈಟ್‌ಗಳು ಆಗಾಗ್ಗೆ ಕುಕೀಗಳು ಮತ್ತು ಸ್ಕ್ರಿಪ್ಟ್‌ಗಳನ್ನು ಒಳಗೊಂಡಿರುತ್ತವೆ ಮತ್ತು ಅವುಗಳು ನಿಮ್ಮನ್ನು ಮತ್ತು ನಿಮ್ಮ ಸಾಧನವನ್ನು ಗುರುತಿಸಲು ಪ್ರಯತ್ನಿಸುತ್ತವೆ. ನೀವು ಯಾರೆಂದು ಪತ್ತೆಹಚ್ಚಲು ಅವುಗಳು ಬಯಸುತ್ತವೆ ಮತ್ತು ಪ್ರತಿ ಸೈಟ್‌ನಲ್ಲಿ ನೀವು ಏನನ್ನು ಮಾಡುತ್ತೀರೆಂದು ವೆಬ್‌ನಾದ್ಯಂತ ನಿಮ್ಮನ್ನು ಅನುಸರಣೆ ಮಾಡುತ್ತವೆ.\n\nಈ ಸಂಗತಿಗಳನ್ನು Brave ನಿರ್ಬಂಧಿಸುತ್ತದೆ ಮತ್ತು ಈ ಮೂಲಕ ನೀವು ಅನುಸರಣೆ ಮಾಡಲ್ಪಡದೇ ಬ್ರೌಸ್ ಮಾಡಬಹುದು.</translation>
<translation id="8553998445606452168">Brave Shields ಕುರಿತು</translation>
<translation id="8912304686498704084">ಮೇಲಕ್ಕೆ</translation>
<translation id="4963218453264888510">ಕೆಳಗೆ</translation>
<translation id="7474113515179313724">Brave ನ ಗೌಪ್ಯತೆ ರಕ್ಷಣೆಗಳಿಲ್ಲದೆ ನೀವು ಈ ಸೈಟ್‌ ಅನ್ನು ಬ್ರೌಸ್ ಮಾಡುತ್ತಿದ್ದೀರಿ. ಇದು ಶಿಲ್ಡ್ಸ್ಅಪ್ ಜೊತೆಗೆ ಸರಿಯಾಗಿ ಕಾರ್ಯನಿರ್ವಹಿಸುವುದಿಲ್ಲವೇ?</translation>
<translation id="9055546041146396417">ಈ ಸೈಟ್‌ಗೆ ಹಾನಿಯಾದರೆ, ಶೀಲ್ಡ್‌ಗಳನ್ನು ಕೆಳಕ್ಕೆ ಮಾಡಲು ಪ್ರಯತ್ನಿಸಿ.</translation>
<translation id="1209739344687072997">ಬ್ರೋಕನ್ ಸೈಟ್ ಅನ್ನು ವರದಿ ಮಾಡಿ</translation>
<translation id="8510478288840333677">ಶೀಲ್ಡ್ ನೊಂದಿಗೆ ಈ ಸೈಟ್ ಸರಿಯಾಗಿ ಕಾರ್ಯನಿರ್ವಹಿಸುವುದಿಲ್ಲ ಎಂದು Brave ನ ಡೆವಲಪರ್ಗಳಿಗೆ ತಿಳಿಸಿ:</translation>
<translation id="5429200298326665264">ಸೂಚನೆ: ಈ ಸೈಟ್ ವಿಳಾಸವನ್ನು, ನಿಮ್ಮ Brave ಆವೃತ್ತಿ ಸಂಖ್ಯೆ ಮತ್ತು ನಿಮ್ಮ ಐಪಿ ವಿಳಾಸದೊಂದಿಗೆ ಸಲ್ಲಿಸಲಾಗುತ್ತದೆ (ಅದನ್ನು ಸಂಗ್ರಹಿಸಲಾಗುವುದಿಲ್ಲ).</translation>
<translation id="5299848999918506028">ಧನ್ಯವಾದಗಳು!</translation>
<translation id="76550953340008122">ಈ ಸೈಟ್‌ನಲ್ಲಿ ಯಾವುದೋ ದೋಷವಿದೆ ಎಂದು Brave ನ ಡೆವಲಪರ್‌ಗಳಿಗೆ ತಿಳಿಸಿದ್ದಕ್ಕಾಗಿ ಧನ್ಯವಾದಗಳು. ಅದನ್ನು ಸರಿಪಡಿಸಲು ನಾವು ನಮ್ಮ ಕೈಲಾದ ಮಟ್ಟಿಗೆ ಪ್ರಯತ್ನಿಸುತ್ತೇವೆ!</translation>
<translation id="650318023204990401">ಸುಧಾರಿತ ನಿಯಂತ್ರಣಗಳು</translation>
<translation id="1275664486910646411">ಪ್ರಮುಖ ಸೈಟ್‌ಗಳನ್ನು ಸ್ವಯಂಪೂರ್ಣಗೊಳಿಸುವಿಕೆಯಲ್ಲಿ ತೋರಿಸಿ</translation>
<translation id="5516010970279932133">ಸೂಚಿಸಿದ ಸೈಟ್‌ಗಳನ್ನು ಸ್ವಯಂಪೂರ್ಣಗೊಳಿಸುವಿಕೆಯಲ್ಲಿ ತೋರಿಸಿ</translation>
<translation id="5370478469168390143">ಡೇಟಾ ಆನ್‌ಲೈನ್ ಉಳಿಸಿ</translation>
<translation id="1118508688433541158">Brave ನೊಂದಿಗೆ ವೇಗವಾಗಿ ಬ್ರೌಸ್ ಮಾಡಿ ಮತ್ತು ಜಾಹೀರಾತುಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ</translation>
<translation id="7427391215172269363">ನಾನು ಪ್ರತಿನಿತ್ಯ Brave ಮೂಲಕ ವೆಬ್ ಬ್ರೌಸ್ ಮಾಡಿ ಡೇಟಾ ಉಳಿಸುತ್ತೇನೆ.</translation>
<translation id="4776948189086901857">Brave ಪ್ರತಿಯೊಂದು ಕಡೆ ಕಾರ್ಯನಿರ್ವಹಿಸುತ್ತದೆ</translation>
<translation id="1606322780901645965">ವೇಗವಾದ ಮತ್ತು ಹೆಚ್ಚು ಖಾಸಗಿ ವೆಬ್‌ಗಾಗಿ ನಿಮ್ಮ ಲ್ಯಾಪ್‌ಟಾಪ್‌ನಲ್ಲಿ ಅಥವಾ ಟ್ಯಾಬ್ಲೆಟ್‌ನಲ್ಲಿ brave.com ನಲ್ಲಿ Brave ಅನ್ನು ಡೌನ್‌ಲೋಡ್ ಮಾಡಿ</translation>
<translation id="8374242949413616101">ಗೌಪ್ಯತೆ ರಕ್ಷಣೆ</translation>
<translation id="4955406228638026265">ನಿಮ್ಮ ಸಮ್ಮತಿ ಇಲ್ಲದೆಯೇ ನಿಮ್ಮ ಕುರಿತಾದ ಮಾಹಿತಿಯನ್ನು ಸಂಗ್ರಹಿಸದಂತೆ ಜಾಹೀರಾತುಗಳು ಮತ್ತು ಟ್ರ್ಯಾಕರ್‌ಗಳನ್ನು Brave ನಿಲ್ಲಿಸುತ್ತದೆ.</translation>
<translation id="752842701893882688">ಡೇಟಾ ಮತ್ತು ಬ್ಯಾಟರಿ ಉಳಿಸಿ</translation>
<translation id="1685837926613452362">ಜಾಹೀರಾತುಗಳು ಮತ್ತು ಟ್ರ್ಯಾಕರ್‌ಗಳಿಲ್ಲದೇ, ವೆಬ್‌ಸೈಟ್‌ಗಳು ಕಡಿಮೆ ಡೇಟಾ ಮತ್ತು ಬ್ಯಾಟರಿ ಬಳಸುತ್ತವೆ. ಕ್ಯಾರಿಯರ್ ಶುಲ್ಕಗಳಲ್ಲಿ ಸಹ ನೀವು ಹಣವನ್ನು ಉಳಿಸಬಹುದು.</translation>
<translation id="5440744330377972191">ವೆಬ್‌ಸೈಟ್‌ಗಳು ವೇಗವಾಗಿ ಲೋಡ್ ಆಗುತ್ತವೆ</translation>
<translation id="3000738865641747653">ಕಡಿಮೆ ಡೇಟಾ ಲೋಡ್ ಮಾಡುವುದು ಎಂದರೆ Brave ನಲ್ಲಿನ ವೆಬ್ಸೈಟ್ಗಳು ಹೆಚ್ಚು ವೇಗವಾಗಿ - ಇತರವುಗಳಿಗಿಂತ 8x ವೇಗವಾಗಿ ಲೋಡ್ ಆಗುತ್ತವೆ ಎಂದರ್ಥ.</translation>
<translation id="8903462729414407077">ಸಾಪ್ತಾಹಿಕ ನವೀಕರಣಗಳನ್ನು ಪಡೆಯಿರಿ</translation>
<translation id="7450323860390658083">ನೀವು ಭೇಟಿ ನೀಡುವ ಯಾವ ವೆಬ್‌ಸೈಟ್‌ಗಳು ಗೌಪ್ಯತೆಗೆ ಹೆಚ್ಚು ಆಕ್ರಮಣಕಾರಿಯಾಗಿವೆ ಎಂಬುದರ ಕುರಿತು ಮಾಹಿತಿ ತಿಳಿದುಕೊಂಡಿರಿ.\n\nಈ ಗೌಪ್ಯತೆ ಅಂಕಿಅಂಶಗಳನ್ನು ನೀವು ಮಾತ್ರ ನೋಡಬಹುದು. ಯಾವುದೇ ವೈಯಕ್ತಿಕ ಡೇಟಾ ಅಥವಾ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸವು ಎಂದಿಗೂ ನಿಮ್ಮ ಸಾಧನದಲ್ಲಿ Brave ಬ್ರೌಸರ್ ತೊರೆಯವುದಿಲ್ಲ.</translation>
<translation id="6170159749517116894">ಗೌಪ್ಯತೆ ವರದಿಗಳನ್ನು ಆನ್ ಮಾಡಿ</translation>
<translation id="5966707198760109579">ವಾರ</translation>
<translation id="8987927404178983737">ತಿಂಗಳು</translation>
<translation id="4302754476737420523">3 ತಿಂಗಳುಗಳು</translation>
<translation id="8248593573177859437">ಗೌಪ್ಯತೆ ವರದಿ</translation>
<translation id="1274039301958114163">ಗೌಪ್ಯತೆ ವರದಿ ಅಧಿಸೂಚನೆ</translation>
<translation id="453601949003770390">ಟ್ರ್ಯಾಕ್‌ಗಳು &amp; ಜಾಹೀರಾತುಗಳನ್ನು ನಿರ್ಬಂಧಿಸಲಾಗಿದೆ</translation>
<translation id="1581614543353331529">ತೋರಿಸಲು ಇನ್ನೂ ಯಾವುದೇ ಡೇಟಾ ಇಲ್ಲ</translation>
<translation id="2401984885421380890">ಟ್ರ್ಯಾಕರ್‌ಗಳು ಮತ್ತು ಜಾಹೀರಾತುಗಳನ್ನು\nನಿರ್ಬಂಧಿಸಲಾಗಿದೆ</translation>
<translation id="2662183408296991265">ಡೇಟಾ ಉಳಿಸಿರುವುದು\n(<ph name="DATA_SAVED"/>)</translation>
<translation id="3180928261057583677">ಡೇಟಾ ಉಳಿಸಲಾಗಿದೆ (<ph name="DATA_SAVED"/>)</translation>
<translation id="6814582232712814369">ಉಳಿಸಲಾದ ಸಮಯ</translation>
<translation id="3694122362646626770">ವೆಬ್‌ಸೈಟ್‌ಗಳು</translation>
<translation id="2225863440766607138">ಟ್ರ್ಯಾಕರ್‌ಗಳು</translation>
<translation id="2993367530362078067"><ph name="COUNT"/><ph name="COUNT_TEXT"/></translation>
<translation id="6737820963776538947">ನೀವು ಕಳೆದ 3 ಗಂಟೆಗಳಲ್ಲಿ Brave ಬ್ರೌಸರ್ ಜೊತೆಗೆ <ph name="COUNT"/> ಟ್ರ್ಯಾಕರ್‌ಗಳು ಮತ್ತು ಜಾಹೀರಾತುಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿದ್ದೀರಿ.</translation>
<translation id="3621193901658023971">ಟ್ರ್ಯಾಕರ್‌ಗಳು ಮತ್ತು ಜಾಹೀರಾತುಗಳನ್ನು ನಿರ್ಬಂಧಿಸುವ ಮೂಲಕ Brave ನಲ್ಲಿ ಪ್ರಮುಖ ಸುದ್ದಿ ಸೈಟ್‌ಗಳಲ್ಲಿ 6x ವೇಗವಾಗಿ ಬ್ರೌಸ್ ಮಾಡಿ.</translation>
<translation id="7427567994475794038">ಕಳೆದ 3 ಗಂಟೆಗಳಲ್ಲಿ ಎಷ್ಟು ಟ್ರ್ಯಾಕರ್‌ಗಳು ಮತ್ತು ಜಾಹೀರಾತುಗಳನ್ನು ನಿರ್ಬಂಧಿಸಲಾಗಿದೆ ಎಂಬುದನ್ನು ನೋಡಲು ಗೌಪ್ಯತೆ ವರದಿಗಳನ್ನು ಆನ್ ಮಾಡಿ.</translation>
<translation id="2053891538879944400">ನೀವು Brave ಬ್ರೌಸರ್ ಮೂಲಕ <ph name="DATA"/> <ph name="DATA_TEXT"/> ಡೇಟಾ ಉಳಿಸಿದ್ದೀರಿ.</translation>
<translation id="8640064498452534195">ನೀವು ಎಷ್ಟು ಡೇಟಾ ಉಳಿಸಿದ್ದೀರಿ ಎಂಬುದನ್ನು ನೋಡಲು ಗೌಪ್ಯತೆ ವರದಿಗಳನ್ನು ಆನ್ ಮಾಡಿ.</translation>
<translation id="2252497442547961651">ಸರಾಸರಿಯಾಗಿ Brave ಬ್ರೌಸರ್ 40% ಕಡಿಮೆ ಬ್ಯಾಟರಿ ಬಳಸುತ್ತದೆ.🔋</translation>
<translation id="1945330434665345785">ನೀವು ಕಳೆದ ವಾರ Brave ಬ್ರೌಸರ್ ಮೂಲಕ <ph name="COUNT"/> ಟ್ರ್ಯಾಕರ್‌ಗಳು ಮತ್ತು ಜಾಹೀರಾತುಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿದ್ದೀರಿ.</translation>
<translation id="2883868922264431515">ಗೌಪ್ಯತೆ-ಗೌರವಿಸುವ ಜಾಹೀರಾತುಗಳನ್ನು ವೀಕ್ಷಿಸುವ ಮೂಲಕ ಟೋಕನ್‌ಗಳನ್ನು ಗಳಿಸಿ ಮತ್ತು ನಿಮ್ಮ ಮೆಚ್ಚಿನ ವೆಬ್‌ಸೈಟ್‌ಗಳನ್ನು ಬೆಂಬಲಿಸಿ.</translation>
<translation id="7408493389591475748">ಗೌಪ್ಯತೆ ವರದಿ ಮಾಡುವಿಕೆಯನ್ನು ನೋಡಲು ಬ್ರೌಸಿಂಗ್ ಆರಂಭಿಸಿ</translation>
<translation id="9080777565335515917">ಖಾಸಗಿ ಮತ್ತು ಸುರಕ್ಷಿತ ಬ್ರೌಸಿಂಗ್‌ಗಾಗಿ Brave ಅನ್ನು ನಿಮ್ಮ ಡೀಫಾಲ್ಟ್ ಬ್ರೌಸರ್ ಆಗಿ ಹೊಂದಿಸಿ.</translation>
<translation id="1390943217516275883">ನಿಮ್ಮ ಗೌಪ್ಯತೆ ಅಂಕಿಅಂಶಗಳನ್ನು ತೋರಿಸಲು Brave ಗೆ ಅನುಮತಿಸುತ್ತದೆ</translation>
<translation id="1763352720905582220">ಸಾಪ್ತಾಹಿಕ ಗೌಪ್ಯತೆ ವರದಿ ಕಳುಹಿಸುತ್ತದೆ</translation>
<translation id="1811691435325545066">ಎಲ್ಲಾ ಗೌಪ್ಯತೆ ವರದಿಗಳ ಡೇಟಾ ತೆರವುಗೊಳಿಸಿ</translation>
<translation id="5966003059662780423">ನಿರ್ಗಮಿಸುವಲ್ಲಿ ತೆರವುಗೊಳಿಸಿ</translation>
<translation id="3980704343258720102">ಎಲ್ಲಾ ಬ್ರೌಸಿಂಗ್ ಡೇಟಾವನ್ನು ತೆರವುಗೊಳಿಸುತ್ತದೆ</translation>
<translation id="8089997572759938512">ನಿಮ್ಮ ಗೌಪ್ಯತೆ ವರದಿಗಳನ್ನು ಶೂನ್ಯಕ್ಕೆ ಮರುಹೊಂದಿಸುತ್ತದೆ</translation>
<translation id="2718046830255553131">ಗೌಪ್ಯತೆ ವರದಿಗಳ ಡೇಟಾವನ್ನು ತೆರವುಗೊಳಿಸಲಾಗಿದೆ.</translation>
<translation id="7559243474426790340">ಸ್ಥಿತಿ ರಿಫ್ರೆಶ್ ಮಾಡಿ</translation>
