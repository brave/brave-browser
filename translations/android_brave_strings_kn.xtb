<translation id="1383876407941801731">ಹುಡುಕಿ</translation>
<translation id="8446884382197647889">ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ</translation>
<translation id="4789438578712143420">≈ $<ph name="USD_BALANCE"/></translation>
<translation id="558614325644656740"><ph name="COIN_BALANCE"/> <ph name="COIN_NAME"/> ಲಭ್ಯವಿದೆ</translation>
<translation id="3414885529641505869"><ph name="COIN_NAME"/> ಖರೀದಿಸಿ</translation>
<translation id="1080611582879541635"><ph name="COIN_ADDRESS"/> ಡೆಪಾಸಿಟ್ ವಿಳಾಸ</translation>
<translation id="4836242416604533644"><ph name="COIN_MEMO"/> ಡೆಪಾಸಿಟ್ ಮೆಮೋ</translation>
<translation id="6755546361496900700">ಪರಿವರ್ತನೆಯನ್ನು ಪೂರ್ವವೀಕ್ಷಣೆ ಮಾಡಿ</translation>
<translation id="5110244215256675209">ಕ್ರಿಪ್ಟೋ ಖರೀದಿಸಿ</translation>
<translation id="2271556362602437160">.com</translation>
<translation id="3727077517986954867">.us</translation>
<translation id="13296465911709339">ನಿಮ್ಮ ಪಠ್ಯವನ್ನು ಕ್ಲಿಪ್‌ಬೋರ್ಡ್‌ಗೆ ನಕಲಿಸಲಾಗಿದೆ.</translation>
<translation id="2735834606413553397">ಸಾರ್ವಜನಿಕ ವಿಳಾಸವನ್ನು ಕ್ಲಿಪ್‌ಬೋರ್ಡ್‌ಗೆ ನಕಲಿಸಲಾಗಿದೆ.</translation>
<translation id="7144878232160441200">ಮರುಪ್ರಯತ್ನಿಸಿ</translation>
<translation id="2977461003134694205">ಗೌಪ್ಯತೆ ಅಂಕಿಅಂಶಗಳು</translation>
<translation id="2956070106555335453">ಸಾರಾಂಶ</translation>
<translation id="1366855498335432549">ಡೆಪಾಸಿಟ್ ಮಾಡಿ</translation>
<translation id="1301340234286078471">ಪರಿವರ್ತಿಸಿ</translation>
<translation id="223536150967420869">ಖರೀದಿಸಿ</translation>
<translation id="4584930231524505442">ನಾನು ಪರಿವರ್ತಿಸಲು ಬಯಸುತ್ತೇನೆ…</translation>
<translation id="5749014321384313843">ನಾನು ಖರ್ಚು ಮಾಡಲು ಬಯಸುತ್ತೇನೆ...</translation>
<translation id="2480674848181564169">ಪರಿವರ್ತಿಸಲು ಸಾಕಷ್ಟು ಬ್ಯಾಲೆನ್ಸ್ ಇಲ್ಲ</translation>
<translation id="2215334405220411393">ಪರಿವರ್ತಿಸಲು ಕನಿಷ್ಠ ಮೊತ್ತ : <ph name="COIN_BALANCE"/> <ph name="COIN_NAME"/></translation>
<translation id="6702658079816271889">ಸಂಪರ್ಕಿಸಲು ಸಾಧ್ಯವಾಗುತ್ತಿಲ್ಲ</translation>
<translation id="7742417737698031726">ಶುಲ್ಕ</translation>
<translation id="2122225204180056042">ನೀವು ಇದನ್ನು ಸ್ವೀಕರಿಸುತ್ತೀರಿ</translation>
<translation id="6432284126416751397">ಪರಿವರ್ತನೆಯನ್ನು ದೃಢೀಕರಿಸಿ</translation>
<translation id="3891414008432200754">ದರ</translation>
<translation id="3733576242118819803">ದೃಢಪಡಿಸಿ (<ph name="REMAINING_TIME"/>ಗಳು)</translation>
<translation id="1627064138060308910">ಪರಿವರ್ತನೆ ವಿಫಲಗೊಂಡಿದೆ. ದಯವಿಟ್ಟು ಮತ್ತೆ ಪ್ರಯತ್ನಿಸಿ.</translation>
<translation id="1449790603979388000"><ph name="COUNT"/> <ph name="COUNT_TEXT"/></translation>
<translation id="7465614181960492526">ಟೋಕನ್‌ಗಳನ್ನು ಗಳಿಸಿ &amp; ಹಿಂದಕ್ಕೆ ನೀಡಿ</translation>
<translation id="244362569344671266">Brave ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳನ್ನು ವೀಕ್ಷಿಸುವ ಮೂಲಕ ಟೋಕನ್‌ಗಳನ್ನು ಸಂಪಾದಿಸಿ ಮತ್ತು ವಿಷಯ ರಚನೆಕಾರರನ್ನು ಸ್ವಯಂಚಾಲಿತವಾಗಿ ಬೆಂಬಲಿಸಿ.</translation>
<translation id="631124996535890460">ಮುಂದುವರಿಯುವ ಮೂಲಕ, ನೀವು <ph name="TERMS_OF_SERVICE"/> ಮತ್ತು <ph name="PRIVACY_POLICY"/> ಗೆ ಸಮ್ಮತಿಸಿದ್ದೀರಿ.</translation>
<translation id="8804164990146287819">ಗೌಪ್ಯತೆ ನೀತಿ</translation>
<translation id="6318696841371191107">ತ್ವರಿತ ಪ್ರವಾಸ ಮಾಡಿ</translation>
<translation id="7930505144430343145">Brave Rewards ಗೆ ಸ್ವಾಗತ!</translation>
<translation id="3477340970981564961">ನಿಮ್ಮ ವೈಯಕ್ತಿಕ ಮಾಹಿತಿಯನ್ನು ಖಾಸಗಿಯಾಗಿಟ್ಟುಕೊಂಡು ಸಾಂಪ್ರದಾಯಿಕ ಜಾಹೀರಾತುಗಳಿಗಿಂತ ಭಿನ್ನವಾಗಿ ವಿಷಯ ರಚನೆಕಾರರನ್ನು ಬೆಂಬಲಿಸಲು Brave ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳು ನಿಮಗೆ ಟೋಕನ್‌ಗಳನ್ನು ನೀಡುತ್ತವೆ.</translation>
<translation id="6973095375773523904">ಜಾಹೀರಾತುಗಳು ಎಲ್ಲಿ ತೋರಿಸುತ್ತವೆ?</translation>
<translation id="7556325460112047619">Brave ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳು ಸಾಮಾನ್ಯ ಅಧಿಸೂಚನೆಯಂತೆ ಗೋಚರಿಸುತ್ತವೆ. ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ಈ ಜಾಹೀರಾತುಗಳನ್ನು ನೀವು ಎಷ್ಟು ಬಾರಿ ನೋಡುತ್ತೀರಿ ಎಂಬುದನ್ನು ನೀವು ನಿಯಂತ್ರಿಸುತ್ತೀರಿ.</translation>
<translation id="6897083627729802351">ನೀವು ಯಾವಾಗ ರಿವಾರ್ಡ್‌ಗಳನ್ನು ಪಡೆಯುತ್ತೀರಿ?</translation>
<translation id="310863932297733723">ಪ್ರಸ್ತುತ ತಿಂಗಳಿನಾದ್ಯಂತ Brave ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳಿಂದ ನೀವು ಗಳಿಸಿದ ಟೋಕನ್‌ಗಳು ಮುಂದಿನ ತಿಂಗಳು 5 ರಂದು ಬರಲಿವೆ.</translation>
<translation id="7712287524451429139">ಮರಳಿ ಹಿಂತಿರುಗಿಸುವುದು ಪ್ರಯತ್ನರಹಿತವಾಗಿದೆ</translation>
<translation id="105037569026952831">ನಾವು "ಗಮನಿಸುವಿಕೆ" ಎಂಬುದಾಗಿ ಅಳೆಯುವ ನಿಮ್ಮ ತೊಡಗಿಸಿಕೊಳ್ಳುವಿಕೆಯ ಹಂತಗಳ ಆಧಾರದ ಮೇಲೆ ರಚನೆಕಾರರು ನಿಮ್ಮ ಟೋಕನ್ ಕೊಡುಗೆಗಳನ್ನು ಸ್ವಯಂಚಾಲಿತವಾಗಿ ಸ್ವೀಕರಿಸುತ್ತಾರೆ.</translation>
<translation id="5323267338679539010">ಸಲಹೆಗಳೊಂದಿಗೆ ಧನ್ಯವಾದಗಳು ಎಂದು ಹೇಳಿ</translation>
<translation id="6730444165801605682">ಟಿಪ್ಪಿಂಗ್ ಎಂಬುದು ನೀವು ಸಂಪೂರ್ಣವಾಗಿ ಇಷ್ಟಪಡುವ ವಿಷಯ ಅಥವಾ ರಚಿಸುವವರನ್ನು ವೈಯಕ್ತಿಕವಾಗಿ ಪ್ರೋತ್ಸಾಹಿಸುವ ಮತ್ತು ಬೆಂಬಲಿಸುವ ಒಂದು ಮಾರ್ಗವಾಗಿದೆ. ಟಿಪ್ಪಿನ್ ಅನ್ನು ಪಡೆಯಿರಿ!</translation>
<translation id="7509452925943421624">ನೀವು ಟೋಕನ್‌ಗಳೊಂದಿಗೆ ಏನು ಮಾಡಬಹುದು?</translation>
<translation id="5876169128756792273">ರಚನೆಕಾರರನ್ನು ಬೆಂಬಲಿಸುವುದನ್ನು ಮೀರಿ ಟೋಕನ್‌ಗಳನ್ನು ಬಳಸಬಹುದು. ವ್ಯಾಪಾರಿಗಳು ನಮ್ಮ ವೇದಿಕೆಯನ್ನು ಸೇರಿಕೊಂಡಂತೆಲ್ಲಾ ನೀವು ಡಿಜಿಟಲ್ ವಿಷಯ ಮತ್ತು ಇತರ ಸರಕುಗಳನ್ನು ಖರೀದಿಸಬಹುದು.</translation>
<translation id="1402530520078693620">ಕೊನೆಯದಾಗಿ, ಮೂಲ ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ನಿಮಗೆ ಹೊಂದಿಸಲು ಅವಕಾಶ ಮಾಡಿಕೊಡಿ</translation>
<translation id="3494356595346329740">ನೀವು ಇದನ್ನು ನಂತರ ಬದಲಾಯಿಸಬಹುದು.</translation>
<translation id="3115702351969988294">ನೀವು ಎಷ್ಟು ಬಾರಿ ಜಾಹೀರಾತುಗಳನ್ನು ನೋಡಲು ಬಯಸುತ್ತೀರಿ?</translation>
<translation id="3474670199029510295">(ಪ್ರತಿ ಗಂಟೆಯ ಜಾಹೀರಾತುಗಳು)</translation>
<translation id="66599604460581068">ನಿಮ್ಮ ನೆಚ್ಚಿನ ರಚನೆಕಾರರಿಗೆ ಎಷ್ಟು ಬೆಂಬಲ ನೀಡಲು ಬಯಸುತ್ತೀರಿ?</translation>
<translation id="3164498053837913829">(ನಿಮ್ಮ ಜಾಹೀರಾತು ಗಳಿಕೆಗಳಿಂದ)</translation>
<translation id="827315274423079662">ಓಹೋ, ನೀವು ಸಿದ್ಧರಾಗಿರುವಿರಿ!</translation>
<translation id="3895246934788932488">Brave Rewards ಬಳಸುವ ಮೂಲಕ, ನಿಮ್ಮ ಗೌಪ್ಯತೆಯನ್ನು ನೀವು ರಕ್ಷಿಸುತ್ತಿದ್ದೀರಿ ಮತ್ತು ಎಲ್ಲರಿಗೂ ವೆಬ್ ಅನ್ನು ಉತ್ತಮ ಸ್ಥಳವನ್ನಾಗಿ ಮಾಡಲು ಸಹಾಯ ಮಾಡುತ್ತಿದ್ದೀರಿ ಮತ್ತು ಅದು ಅದ್ಭುತವಾಗಿದೆ!</translation>
<translation id="5705882733397021510">ಹಿಂತಿರುಗಿ</translation>
<translation id="3985567919248057638">ತ್ವರಿತ ಪ್ರಯಾಣ ಕೈಗೊಳ್ಳೋಣ</translation>
<translation id="2434574859640694129">ನಿಮ್ಮ ಮೊದಲ ಜಾಹೀರಾತುಗಳ ಪಾವತಿಯನ್ನು ನೀವು ಸ್ವೀಕರಿಸಿದ ನಂತರ ನೀವು ಹಿಂತಿರುಗಬಹುದು ಮತ್ತು ಈ ರಚಿಸುವವರಿಗೆ ಸಲಹೆ ನೀಡಬಹುದು!</translation>
<translation id="207242986925259074">ತ್ವರಿತ ರಿಫ್ರೆಶ್ ಪ್ರವಾಸ</translation>
<translation id="8342862030112839977">ನಿಮ್ಮ 1 ನೇ ರಿವಾರ್ಡ್‌ಗಳ ಪಾವತಿಯು ಇವರಿಂದ ಬರುತ್ತದೆ:</translation>
<translation id="3769645103175790584">ನೀವು ಬ್ರೌಸ್ ಮಾಡುವಾಗ ಟೋಕನ್‌ಗಳನ್ನು ಸಂಪಾದಿಸಿ ಮತ್ತು ಉತ್ತಮ ವಿಷಯಕ್ಕಾಗಿ ರಚನೆಕಾರರಿಗೆ ರಿವಾರ್ಡ್ ನೀಡಿ. <ph name="QUICK_TOUR"/>.</translation>
<translation id="6912901278692845878">ತ್ವರಿತ ಪ್ರವಾಸ</translation>
<translation id="1080013038789678738">ನಾನು ಭಾಗವಹಿಸುತ್ತಿದ್ದೇನೆ!</translation>
<translation id="8037420924417098685">ಉಚಿತ ಕರೆಯನ್ನು ಆರಂಭಿಸಲು, Brave ರಿವಾರ್ಡ್ಸ್ ಆನ್ ಮಾಡಿ</translation>
<translation id="8961724619078500274">Brave Rewards ಮೂಲಕ, ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳನ್ನು ವೀಕ್ಷಿಸಲು ಟೋಕನ್‌ಗಳನ್ನು ಗಳಿಸುತ್ತೀರಿ (ನಿಮ್ಮ ಡೇಟಾ ಸುರಕ್ಷಿತವಾಗಿರುತ್ತದೆ). ಆ ಟೋಕನ್‌ಗಳನ್ನು ನಗದು, ಉಡುಗೊರೆ ಕಾರ್ಡ್‌ಗಳಾಗಿ ಪರಿವರ್ತಿಸಿ…ಅಥವಾ ನೀವು ಇಷ್ಟಪಡುವ ವೆಬ್‌ಸೈಟ್‌ಗಳಿಗೆ ಟಿಪ್ ನೀಡಿ. ಟ್ರ್ಯಾಕಿಂಗ್ ಇಲ್ಲ. ಯಾವುದೇ ಸ್ಧಗಿತಗಳಿಲ್ಲ.</translation>
<translation id="7307847920988946894">ರಿವಾರ್ಡ್‌ಗಳನ್ನು ಆನ್ ಮಾಡಿ</translation>
<translation id="7872993190040589450">ಸಕ್ರಿಯಗೊಳಿಸುವ ಮೂಲಕ, ನೀವು <ph name="TERMS_OF_SERVICE"/> ಮತ್ತು <ph name="PRIVACY_POLICY"/> ಗೆ ಒಪ್ಪುತ್ತೀರಿ.</translation>
<translation id="6128511569382736135">ಉಚಿತ ಕರೆಯನ್ನು ಆರಂಭಿಸಲು, Brave ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳು ಅನ್ನು ಸಕ್ರಿಯಗೊಳಿಸಿ</translation>
<translation id="7930006085154756245">Brave ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳು ಮೂಲಕ, ನೀವು ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳನ್ನು ವೀಕ್ಷಿಸಲು ಟೋಕನ್‌ಗಳನ್ನು ಗಳಿಸುತ್ತೀರಿ (ನಿಮ್ಮ ಡೇಟಾ ಸುರಕ್ಷಿತವಾಗಿರುತ್ತದೆ). ಆ ಟೋಕನ್‌ಗಳನ್ನು ನಗದು, ಉಡುಗೊರೆ ಕಾರ್ಡ್‌ಗಳಾಗಿ ಪರಿವರ್ತಿಸಿ…ಅಥವಾ ನೀವು ಇಷ್ಟಪಡುವ ವೆಬ್‌ಸೈಟ್‌ಗಳಿಗೆ ಸಲಹೆ ನೀಡಿ. ಟ್ರ್ಯಾಕಿಂಗ್ ಇಲ್ಲ. ಯಾವುದೇ ಸ್ಧಗಿತಗಳಿಲ್ಲ.</translation>
<translation id="4992617448154987397">Brave ಖಾಸಗಿ ಜಾಹೀರಾತುಗಳನ್ನು ಆನ್ ಮಾಡಿ</translation>
<translation id="3415016869273103668">ನೀವು ಇದೀಗ ಉಚಿತ ಕರೆಯನ್ನು ಆರಂಭಿಸಬಹುದು</translation>
<translation id="7073631403665023390">Brave ಟಾಕ್‌ಗೆ ಮುಂದುವರಿಯಲು ಸ್ಕ್ರೀನ್‌ನಲ್ಲಿ ಎಲ್ಲಿಯಾದರೂ ಕ್ಲಿಕ್ ಮಾಡಿ ಅಥವಾ ಟ್ಯಾಪ್ ಮಾಡಿ.</translation>
<translation id="626339649633171286">Brave ರಿವಾರ್ಡ್ಸ್ ಕುರಿತು ಹೆಚ್ಚು ತಿಳಿಯಲು ಬಯಸುವಿರಾ? <ph name="TAKE_QUICK_TOUR"/>.</translation>
<translation id="3604285424134705930">ನೀವು <ph name="FROM_VALUE"/> <ph name="FROM_CURRENCY"/> ರಿಂದ <ph name="TO_VALUE"/> <ph name="TO_CURRENCY"/> ಗೆ ಪರಿವರ್ತಿಸಿದ್ದೀರಿ!</translation>
<translation id="392587271502873865">ದಯವಿಟ್ಟು ನಿಮ್ಮ ನೆಟ್‌ವರ್ಕ್ ಸಂಪರ್ಕವನ್ನು ಪರಿಶೀಲಿಸಿ.</translation>
